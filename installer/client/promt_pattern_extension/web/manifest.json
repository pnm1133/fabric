{
    "manifest_version": 2,
    "name": "Flutter Text Saver",
    "version": "1.0",
    "description": "A Chrome extension to save and copy text using Flutter.",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "icons/Icon-192.png"
    },
    "icons": {
        "48": "icons/Icon-192.png",
        "128": "icons/Icon-512.png"
    }
}
